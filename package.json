{
  "name": "js-jieba",
  "version": "1.0.12",
  "description": "for js jieba lib",
  "main": "dist/jieba.js",
  "module": "esm.mjs",
  "prettier": "prettier-config-standard",
  "files": [
    "index.js",
    "esm.mjs",
    "dist/*",
    "types/*.d.ts"
  ],
  "directories": {},
  "scripts": {
    "build": "docker-compose run --rm build",
    "build:jieba": "NODE_ENV=production webpack --config webpack/jieba",
    "build:dict": "npm run build:dict:zh-cn;npm run build:zh-tw",
    "build:zh-cn": "make zh-tw",
    "build:zh-tw": "make zh-cn",
    "build:types": "tsc -p tsconfig.types.json",
    "build:web": "npm -C ./packages/web-demo run build",
    "deploy:web": "npm -C ./packages/web-demo run deploy",
    "bump-version": "node scripts/bump-version.js",
    "test": "jest",
    "test-types": "tsd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josudoey/js-jieba.git"
  },
  "author": "Joey Fang",
  "keywords": [
    "analyzer",
    "chinese",
    "chinese segmentation",
    "dict",
    "javascript",
    "jieba",
    "optimizer",
    "pure",
    "pure javascript",
    "segment",
    "segmentation",
    "split",
    "tokenizer",
    "trie",
    "word",
    "中文",
    "中文斷詞",
    "斷詞"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/josudoey/js-jieba/issues"
  },
  "homepage": "https://github.com/josudoey/js-jieba#readme",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/jest": "^30.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.19.0",
    "eslint-plugin-promise": "^7.2.1",
    "jest": "^30.0.0",
    "jieba-zh-cn": "workspace:^1.0.10",
    "jieba-zh-tw": "workspace:^1.0.10",
    "js-jieba": "workspace:^1.0.10",
    "prettier-config-standard": "^7.0.0",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.0",
    "tsd": "^0.20.0",
    "typescript": "^5.8.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  }
}
