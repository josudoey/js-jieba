{
  "name": "js-jieba",
  "version": "1.0.12",
  "description": "for js jieba lib",
  "main": "dist/jieba.js",
  "module": "esm.mjs",
  "files": [
    "index.js",
    "esm.mjs",
    "dist/*",
    "types/*.d.ts"
  ],
  "directories": {},
  "scripts": {
    "build": "docker-compose run --rm build",
    "build:jieba": "NODE_ENV=production webpack --config webpack/jieba",
    "build:dict": "npm run build:dict:zh-cn;npm run build:zh-tw",
    "build:zh-cn": "make zh-tw",
    "build:zh-tw": "make zh-cn",
    "build:types": "tsc -p tsconfig.types.json",
    "bump-version": "node scripts/bump-version.js",
    "test": "jest",
    "test-types": "tsd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josudoey/js-jieba.git"
  },
  "author": "Joey Fang",
  "keywords": [
    "analyzer",
    "chinese",
    "chinese segmentation",
    "dict",
    "javascript",
    "jieba",
    "optimizer",
    "pure",
    "pure javascript",
    "segment",
    "segmentation",
    "split",
    "tokenizer",
    "trie",
    "word",
    "中文",
    "中文斷詞",
    "斷詞"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/josudoey/js-jieba/issues"
  },
  "homepage": "https://github.com/josudoey/js-jieba#readme",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.0",
    "brotli-webpack-plugin": "^1.1.0",
    "eslint": "^7.12.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "jest": "^28.1.0",
    "jieba-zh-cn": "workspace:^1.0.10",
    "jieba-zh-tw": "workspace:^1.0.10",
    "js-jieba": "workspace:^1.0.10",
    "ts-jest": "^28.0.3",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.0",
    "tsd": "^0.20.0",
    "typescript": "^4.6.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  }
}
